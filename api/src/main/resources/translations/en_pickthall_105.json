{"code":200,"status":"OK","data":{"number":105,"name":"\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0627\u0644\u0641\u0650\u064a\u0644\u0650","englishName":"Al-Fil","englishNameTranslation":"The Elephant","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6189,"text":"Hast thou not seen how thy Lord dealt with the owners of the Elephant?","numberInSurah":1,"juz":30,"manzil":7,"page":601,"ruku":547,"hizbQuarter":240,"sajda":false},{"number":6190,"text":"Did He not bring their stratagem to naught,","numberInSurah":2,"juz":30,"manzil":7,"page":601,"ruku":547,"hizbQuarter":240,"sajda":false},{"number":6191,"text":"And send against them swarms of flying creatures,","numberInSurah":3,"juz":30,"manzil":7,"page":601,"ruku":547,"hizbQuarter":240,"sajda":false},{"number":6192,"text":"Which pelted them with stones of baked clay,","numberInSurah":4,"juz":30,"manzil":7,"page":601,"ruku":547,"hizbQuarter":240,"sajda":false},{"number":6193,"text":"And made them like green crops devoured (by cattle)?","numberInSurah":5,"juz":30,"manzil":7,"page":601,"ruku":547,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.pickthall","language":"en","name":"Pickthall","englishName":"Mohammed Marmaduke William Pickthall","format":"text","type":"translation","direction":"ltr"}}}