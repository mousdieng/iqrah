{"code":200,"status":"OK","data":{"number":111,"name":"\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0627\u0644\u0645\u064e\u0633\u064e\u062f\u0650","englishName":"Al-Masad","englishNameTranslation":"The Palm Fibre","revelationType":"Meccan","numberOfAyahs":5,"ayahs":[{"number":6217,"text":"The power of Abu Lahab will perish, and he will perish.","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6218,"text":"His wealth and gains will not exempt him.","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6219,"text":"He will be plunged in flaming Fire,","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6220,"text":"And his wife, the wood-carrier,","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false},{"number":6221,"text":"Will have upon her neck a halter of palm-fibre.","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":553,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.pickthall","language":"en","name":"Pickthall","englishName":"Mohammed Marmaduke William Pickthall","format":"text","type":"translation","direction":"ltr"}}}