{"code":200,"status":"OK","data":{"number":107,"name":"\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0627\u0644\u0645\u064e\u0627\u0639\u064f\u0648\u0646\u0650","englishName":"Al-Maa'un","englishNameTranslation":"Almsgiving","revelationType":"Meccan","numberOfAyahs":7,"ayahs":[{"number":6198,"text":"Hast thou observed him who belieth religion?","numberInSurah":1,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6199,"text":"That is he who repelleth the orphan,","numberInSurah":2,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6200,"text":"And urgeth not the feeding of the needy.","numberInSurah":3,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6201,"text":"Ah, woe unto worshippers","numberInSurah":4,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6202,"text":"Who are heedless of their prayer;","numberInSurah":5,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6203,"text":"Who would be seen (at worship)","numberInSurah":6,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false},{"number":6204,"text":"Yet refuse small kindnesses!","numberInSurah":7,"juz":30,"manzil":7,"page":602,"ruku":549,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.pickthall","language":"en","name":"Pickthall","englishName":"Mohammed Marmaduke William Pickthall","format":"text","type":"translation","direction":"ltr"}}}