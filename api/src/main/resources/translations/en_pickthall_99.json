{"code":200,"status":"OK","data":{"number":99,"name":"\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0627\u0644\u0632\u0651\u064e\u0644\u06e1\u0632\u064e\u0644\u064e\u0629\u0650","englishName":"Az-Zalzala","englishNameTranslation":"The Earthquake","revelationType":"Medinan","numberOfAyahs":8,"ayahs":[{"number":6139,"text":"When Earth is shaken with her (final) earthquake","numberInSurah":1,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6140,"text":"And Earth yieldeth up her burdens,","numberInSurah":2,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6141,"text":"And man saith: What aileth her?","numberInSurah":3,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6142,"text":"That day she will relate her chronicles,","numberInSurah":4,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6143,"text":"Because thy Lord inspireth her.","numberInSurah":5,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6144,"text":"That day mankind will issue forth in scattered groups to be shown their deeds.","numberInSurah":6,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6145,"text":"And whoso doeth good an atom's weight will see it then,","numberInSurah":7,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false},{"number":6146,"text":"And whoso doeth ill an atom's weight will see it then.","numberInSurah":8,"juz":30,"manzil":7,"page":599,"ruku":541,"hizbQuarter":239,"sajda":false}],"edition":{"identifier":"en.pickthall","language":"en","name":"Pickthall","englishName":"Mohammed Marmaduke William Pickthall","format":"text","type":"translation","direction":"ltr"}}}