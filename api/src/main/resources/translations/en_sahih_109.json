{"code":200,"status":"OK","data":{"number":109,"name":"\u0633\u064f\u0648\u0631\u064e\u0629\u064f \u0627\u0644\u0643\u064e\u0627\u0641\u0650\u0631\u064f\u0648\u0646\u064e","englishName":"Al-Kaafiroon","englishNameTranslation":"The Disbelievers","revelationType":"Meccan","numberOfAyahs":6,"ayahs":[{"number":6208,"text":"Say, \"O disbelievers,","numberInSurah":1,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false},{"number":6209,"text":"I do not worship what you worship.","numberInSurah":2,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false},{"number":6210,"text":"Nor are you worshippers of what I worship.","numberInSurah":3,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false},{"number":6211,"text":"Nor will I be a worshipper of what you worship.","numberInSurah":4,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false},{"number":6212,"text":"Nor will you be worshippers of what I worship.","numberInSurah":5,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false},{"number":6213,"text":"For you is your religion, and for me is my religion.\"","numberInSurah":6,"juz":30,"manzil":7,"page":603,"ruku":551,"hizbQuarter":240,"sajda":false}],"edition":{"identifier":"en.sahih","language":"en","name":"Saheeh International","englishName":"Saheeh International","format":"text","type":"translation","direction":"ltr"}}}